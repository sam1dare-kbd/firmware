              [
                {
                  "x": 0.5
                },
                "3,0",
                {
                  "x": 0.5,
                  "w": 1.25
                },
                "3,1",
                "3,2",
                "3,3",
                {
                  "x": 0.5,
                  "w": 2.75
                },
                "3,4",
                {
                  "x": 0.5
                },
                "3,6",
                "3,7",
                {
                  "w": 1.25
                },
                "3,8"
                ]  
          ]
    }
}